<body>
	<!-- app -->

<div class="row">
          {{#each products}}
  <div class="col-md-3">
		<section class="section" id="catalog">
			<div class="container-fluid">
				<transition-group>
						<div class="product col-12">
							<div class="product__img">
								<center><img style="height: 140px; width: auto;" src="/product-images/{{this._id}}.jpg" alt=""></center>
							</div>
<h3 style="color: #fb641b; font-size: 20px;" class="mt-3 product__title">{{this.Name}}</h3>
              <h3 class="product__Description">{{this.Description}}</h3>
							<span style="color: #388e3c; font-size: 16px;"  class="product__price"><b>â‚¹ {{this.Price}}</b></span>
							<button onclick="addToCart('{{this._id}}')" class="product__add" type="button">
								<svg xmlns='http://www.w3.org/2000/svg' width='512' height='512' viewBox='0 0 512 512'><path d='M448,256c0-106-86-192-192-192S64,150,64,256s86,192,192,192S448,362,448,256Z' style='fill:none;stroke-miterlimit:10;stroke-width:32px'/><line x1='256' y1='176' x2='256' y2='336' style='fill:none;stroke-linecap:round;stroke-linejoin:round;stroke-width:32px'/><line x1='336' y1='256' x2='176' y2='256' style='fill:none;stroke-linecap:round;stroke-linejoin:round;stroke-width:32px'/></svg>
							</button>
						</div>
				</transition-group>
			</div>
		</section>

	</div>
          {{/each}}
  </div>
</body>






